    "start": "npm run start:server"
  },
  "dependencies": {
    "express": "^4.15.2",
    "node-uuid": "^1.4.8",
    "socket.io": "^1.7.3"
  },
  "devDependencies": {
    "coffee-script": "^1.12.5",
    "lite-server": "^2.3.0",
    "webpack": "^2.4.1"
  }
}
